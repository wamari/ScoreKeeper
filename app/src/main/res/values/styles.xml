<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Primary theme color of the app (sets background color of app bar) -->
        <item name="colorPrimary">#69f0ae</item>
        <item name="colorPrimaryDark">#2bbd7e</item>
        <item name="colorAccent">#9fffe0</item>
    </style>
    <style name="PrimaryButton" parent="Theme.AppCompat">
        <item name="colorButtonNormal">@color/colorPrimary</item>
        <item name="colorControlHighlight">#9fffe0</item>
    </style>
    <style name="ResetButton" parent="Theme.AppCompat">
        <item name="colorButtonNormal">#c50e29</item>
        <item name="colorControlHighlight">#ff867f</item>
    </style>
</resources>
